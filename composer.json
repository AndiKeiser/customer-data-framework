{
    "name": "pimcore/customer-management-framework-bundle",
    "license": "GPL-3.0-or-later",
    "type": "pimcore-bundle",
    "config": {
        "sort-packages": true
    },
    "require": {
        "drewm/mailchimp-api": "*",
        "elements/object-merger-bundle": "^1.0",
        "elements/export-toolkit-bundle": "^2.0",
        "guzzlehttp/guzzle": ">=4.1.4 <7.0",
        "hwi/oauth-bundle": "^0.5.3",
        "mtdowling/cron-expression": "^1.1.0",
        "pimcore/search-query-parser": "^1.2.4",
        "box/spout": "^2.7"
    },
    "require-dev": {
        "codeception/codeception": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "CustomerManagementFrameworkBundle\\": "src/",
            "Pimcore\\": "core-extension/Pimcore/"
        },
        "exclude-from-classmap": [
            "frontend/",
            "frontend-samples/",
            "install/"
        ]
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "CustomerManagementFrameworkBundle\\PimcoreCustomerManagementFrameworkBundle"
            ]
        }
    },
    "repositories": {
        "elements": {
            "type": "composer",
            "url": "https://composer-packages.elements.at/"
        }
    }
}
